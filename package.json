{
  "name": "ds-design",
  "productName": "ds-design",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "wangnengjie",
    "email": "751614701@qq.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ds_design"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@electron-forge/cli": "6.0.0-beta.49",
    "@electron-forge/maker-deb": "6.0.0-beta.49",
    "@electron-forge/maker-rpm": "6.0.0-beta.49",
    "@electron-forge/maker-squirrel": "6.0.0-beta.49",
    "@electron-forge/maker-zip": "6.0.0-beta.49",
    "@electron-forge/plugin-webpack": "6.0.0-beta.49",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/bindings": "^1.3.0",
    "@types/fs-extra": "^8.1.0",
    "@types/nanoid": "^2.1.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.0.0",
    "electron": "^9.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.23.1",
    "typescript": "^3.7.0",
    "url-loader": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "antd": "^3.26.11",
    "bindings": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^8.1.0",
    "nanoid": "^2.1.11",
    "node-addon-api": "^2.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  }
}
